{
    "version": 2,
    "builds": [
      {
        "src": "./api/myFunction.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "env": {
      "MYSQL_HOST": "localhost",
      "MYSQL_USER": "root",
      "MYSQL_PASSWORD": "",
      "MYSQL_DATABASE": "moneymingle"
    },
    "functions": {
      "api/**/*.js": {
        "runtime": "nodejs14.x",
        "maxDuration": 30
      }
    }
  }
  